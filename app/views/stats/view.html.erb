<h1>Listing stats for <%=@date%></h1>
<table border=1 id="stat_list">
  <tr>
  	<th>User</th>
    <th>Scan</th>
    <th>Video </th>
    <th>Macs</th>
    <th>SA Short</th>
    <th>SA Long</th>
    <th>GU</th>
    <th>LL</th>
	<th>Ref</th>
    <th>Comment</th>
    <th>Date</th>
	<th>Time</th>
	<th>EOD Door</th>
  </tr>

<% @stats.each do |stat| %>
  <%= render :partial => 'stat_row', :locals => {:stat => stat} %>
<% end %>
</table>